<section class="todoapp">
  <header class="header">
    <h1>todos</h1>
    <input class="new-todo" placeholder="What needs to be done?" autofocus onkeyup="enterSubmit()">
  </header>
  <section class="main">
    <input id="toggle-all" class="toggle-all" type="checkbox">
    <label for="toggle-all">Mark all as complete</label>
    <ul class="todo-list">
    </ul>
  </section>
  <footer class="footer">
    <span class="todo-count"></span>
    <ul class="filters">
      <li>
        <a href="#" class="selected" onclick="loadList('all')">All</a>
      </li>
      <li>
        <a href="#" onclick="loadList('active')">Active</a>
      </li>
      <li>
        <a href="#" onclick="loadList('completed')">Completed</a>
      </li>
    </ul>
    <button class="clear-completed" onclick="clearCompleted()">Clear completed</button>
  </footer>
</section>
<footer class="info">
  <p>Double-click to edit a todo</p>
  <p>Created by <a href="https://github.com/ralixjs">Ralix Team</a></p>
  <p>Part of <a href="http://todomvc.com">TodoMVC</a></p>
</footer>
